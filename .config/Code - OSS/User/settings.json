{
    "terminal.integrated.persistentSessionReviveProcess": "onExitAndWindowClose",
    "workbench.preferredHighContrastColorTheme": "Default Dark+",
    "tabnine.experimentalAutoImports": true,
    "workbench.startupEditor": "none",
    "editor.wordWrap": "on",
    "editor.accessibilitySupport": "off",
    "security.workspace.trust.untrustedFiles": "open",
    "[python]": {
        "editor.formatOnType": true
    },
    "workbench.colorTheme": "Visual Studio 2017 Dark - C++",
    "explorer.confirmDelete": false,
    "python.defaultInterpreterPath": "/bin/python3",
    "github.copilot.advanced": {},
    "git.confirmSync": false,
    "git.autofetch": true,
    "github.copilot.enable": {
        "*": true,
        "plaintext": false,
        "markdown": true,
        "scminput": false,
        "c": false,
        "cpp": false,
        "tex": false,
        "latex": false
    },
    "cmake.showOptionsMovedNotification": false,
    "cmake.configureOnOpen": false,
    "graphviz-interactive-preview.openAutomatically": false,
    "terminal.integrated.defaultProfile.linux": "zsh",
    "workbench.sideBar.location": "right",
    "redhat.telemetry.enabled": false,
    "workbench.iconTheme": "material-icon-theme",
    "[xml]": {
        "editor.defaultFormatter": "redhat.vscode-xml"
    },
    "indentRainbow.colors": [
        "rgba(255,255,64,0.07)",
        "rgba(127,255,127,0.07)",
        "rgba(255,127,255,0.07)",
        "rgba(79,236,236,0.07)"
    ],
    "notebook.editorOptionsCustomizations": {
        "editor.tabSize": 4,
        "editor.indentSize": 4,
        "editor.insertSpaces": true
    },
    "explorer.confirmDragAndDrop": false,
    "git.suggestSmartCommit": false,
    "github.copilot.editor.enableAutoCompletions": true,
    "update.showReleaseNotes": false,
    "git.openRepositoryInParentFolders": "never",
    "remote.autoForwardPortsSource": "hybrid",
    "workbench.list.mouseWheelScrollSensitivity": 3,
    "editor.mouseWheelScrollSensitivity": 3,
    "security.allowedUNCHosts": [
        "wsl.localhost"
    ],
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    "makefile.configureOnOpen": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.inlineSuggest.edits.allowCodeShifting": "horizontal"
}